        <script type="text/javascript">
            function validateValues() {
                var x=$("#contact").val();
                if (x==null || x==""){
                    alert("Please Enter any contact detail where we can contact you.");
                    return false;
                }
                var y=$("#message").val();
                if (y==null || y==""){
                    alert("Please Leave a Message.");
                    return false;
                }
                return(true);
            }
        </script>
		
		

